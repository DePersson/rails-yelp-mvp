<nav class="navbar navbar-dark bg-dark">
  <span class="navbar-brand mb-0 h1"><h1>Add a new restaurant</h1></span>
</nav>

<div class="container">
  <div class="card">
    <%= simple_form_for [@restaurant] do |f| %>
      <%= f.input :name %>
      <%= f.input :address %>
      <%= f.input :category %>
      <%= f.submit "add a review", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
